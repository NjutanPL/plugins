command /wyjebane [<text>]:
	trigger:
		if {wyjebane::%player%} = true:
			set {wyjebane::%player%} to false
			send "&cWylaczyles &7mozliwosc pisanie do Ciebie."
			stop
		else:
			set {wyjebane::%player%} to true
			send "&aWlaczyles &7mozliwosc pisanie do Ciebie."
			stop
            
            
            
command /msg [<offlineplayer>] [<text>]:
	trigger:
		if arg-1 is set:
			if arg-1 is online:
				if {wyjebane::%arg-1%} is true:
					if arg-2 is set:
						send "&8[&7%player% &3» &7Ty&8] &6%arg-2%" to arg-1
						send "&8[&7Ty &3» &7%arg-1%&8] &6%arg-2%"
						set {%player's uuid%::reply} to arg-1
						set {%uuid of arg-1%::reply} to player
					else:
						send "&aWpisz: &2/msg [gracz] [wiadomosc]"
				else:
					send "&cBlad: &7Ten gracz ma wylaczone msg!"
			else:
				send "&cBlad: &7Podany gracz jest offline."
		else:
			send "&aWpisz: &2/msg [gracz] [wiadomosc]"
            
            
            
            
            



command /ac [<text>]:
        trigger:
                if player has permission "admin.chat":
                        if arg is not set:
                                send "&8» &7Poprawne uzycie: &3/ac [wiadomosc]"
                        else:
                                loop all players:
                                        loop-player has permission "admin.chat":
                                                send "&8» &4[AdminCHAT] &c%player%&8: &7%colored arg%" to loop-player
                else:
                        send "Nie jestes adminem"