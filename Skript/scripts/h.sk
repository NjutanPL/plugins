variables:
	{is::%player%} = true
	{wyjebane::%player%} = true
	{slowmode::helpop::%player%} = 0  
    
on load:
	loop 54 times:
		add 1 to {_a}
		"&2CHAT CONFIG".Inventory.removeItem{{_a}};
		"&2IS".Inventory.removeItem{{_a}};
		"&9Czy aby, napewno?".Inventory.removeItem{{_a}};


command /helpop [<text>]:
	trigger:
		if arg-1 is set:
			if player has permission "admin.helpop":
				set {slowmode::helpop::%player%} to 0
				loop all players in world "world":
					if player are in region "spawn":
						set {_} to "(spawn_1)"
					else:
						set {_} to "(server_1)"
				loop all players:
					if loop-player has permission "admin.helpop":
						send "&7[&cHelpOP&7] &7%{_}% &8%player% &9» &f%arg-1%" to loop-player
				send "&9Wiadomosc do administacjizostala wyslana!"
				stop
			if {slowmode::helpop::%player%} = 0:
				set {slowmode::helpop::%player%} to 15
				loop all players in world "world":
					if player are in region "spawn":
						set {_} to "(spawn_1)"
					else:
						set {_} to "(sv_1)"
				loop all players:
					if loop-player has permission "admin.helpop":
						send "&7[&cHelpOP&7] &7%{_}% &8%player% &9» &f%arg-1%" to loop-player
				send "&9Wiadomosc do administacjizostala wyslana!"
				stop
			else:
				send "&9Nie mozesz pisac tak szybko! Zaczekaj %{slowmode::helpop::%player%}%sek."

every 1 seconds:
	loop all players:
		if {slowmode::helpop::%loop-player%} > 0:
			remove 1 from {slowmode::helpop::%loop-player%}
                
                
                
                              
                
                
                
                
                
command /cc [<text>]:
	trigger:
		if arg-1 is not set:
			if {is::%player%} is true:
				set {_} to "&aWLACZONE"
			else:
				set {_} to "&cWYLACZONE"
			if {wyjebane::%player%} = true:
				set {_1} to "&aWLACZONE"
			else:
				set {_1} to "&cWYLACZONE"
			new Inventory "&2CHAT CONFIG";
			"&2CHAT CONFIG".Inventory.setRow{1};
			"&2CHAT CONFIG".Inventory.setItem{1, 1 paper named "&2Powiadomienia z ItemShop'u!" with lore " &9» &2Aktualnie: %{_}%&2."};
			"&2CHAT CONFIG".Inventory.setCommand{1, "cc is"};
			"&2CHAT CONFIG".Inventory.setExecutor{1, "player"};
			"&2CHAT CONFIG".Inventory.setItem{2, 1 paper named "&2Wiadomosci prywatne!" with lore " &9» &2Aktualnie: %{_1}%&2."};
			"&2CHAT CONFIG".Inventory.setCommand{2, "cc msg"};
			"&2CHAT CONFIG".Inventory.setExecutor{2, "player"};
			"&2CHAT CONFIG".Inventory.display{player};
		if arg-1 is "is":
			if {is::%player%} is true:
				set {is::%player%} to false
				execute player command "cc"
				send "&cWylaczyles &2powiadomienia przy zakupie do siebie!"
				stop
			else:
				set {is::%player%} to true
				execute player command "cc"
				send "&aWlaczyles &2powiadomienia przy zakupie do siebie!"
				stop
		if arg-1 is "msg":
			if {wyjebane::%player%} = true:
				set {wyjebane::%player%} to false
				execute player command "cc"
				send "&cWylaczyles &2prywatne pisanie do siebie!"
				stop
			else:
				set {wyjebane::%player%} to true
				execute player command "cc"
				send "&aWlaczyles &2prywatne pisanie do siebie!"
				stop
command /itemshop [<text>] [<text>] [<integer>]:
	trigger:
		arg-1 is set:
			arg-2 is "vip":
				execute console command "pex user %arg-1% group set vip"
				loop all players:
					if {is::%loop-player%} is true:
						send "&a%arg-1% &2zakupil &arange VIP&7. &a&lDZIEKUJEMY!" to loop-player
			arg-2 is "pandora":
				arg-3 is number:
					add arg-3 to {isPlayer::%arg-1%}
					loop all players:
						if {is::%loop-player%} is true:
							send "&a%arg-1% &2zakupil &apandory x%arg-3%&7. &a&lDZIEKUJEMY!" to loop-player
				else:
					send "&2Wpisz: &a/itemshop [gracz] [vip/pandora] <ilosc>"
			arg-2 is not set:
				send "&2Wpisz: &a/itemshop [gracz] [vip/pandora] <ilosc>"	
		else:
			send "&2Wpisz: &a/itemshop [gracz] [vip/pandora] <ilosc>"
